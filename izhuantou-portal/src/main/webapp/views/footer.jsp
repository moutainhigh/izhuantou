function initBackToTop01 () {
	var $backToTop01 = $(".return_topa");
	$backToTop01.hide();
	
	$(window).scroll(function () {
		if ($(this).scrollTop() > 550) {
			$backToTop01.fadeIn ();
		} else {
			$backToTop01.fadeOut ();
		}
	});
	$backToTop01.click (function (e) {
		e.preventDefault ();
		$('body, html').animate({
			scrollTop: 0
		}, 600);
	});
}
</script>